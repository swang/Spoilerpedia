{
  "name": "Spoilerpedia",
  "version": "0.1.0",
  "description": "Spoilers the last paragraph of any movies on Wikipedia, then loads the 3 most relevant videos related to the ending of the movie",
  "icons": { 
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png" 
  },
  "background": {
    "scripts": ["background.js"]
  },
  
  "permissions": [
    "http://*.wikipedia.org/*",
    "https://*.wikipedia.org/*"
  ],
  "content_scripts": [
		{
			"matches": ["http://*/*","https://*/*"],
			"js": ["contentscript.js"],
			"runat": "document_end",
      "all_frames" : false
		}
  ]
}
